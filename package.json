{
  "name": "@sealsystems/error",
  "version": "2.0.1",
  "description": "Easy handling of error codes and metadata.",
  "author": {
    "name": "SEAL Systems AG",
    "email": "opensource@sealsystems.de",
    "url": "https://www.sealsystems.de"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "semantic-release --branch ${CIRCLE_BRANCH}",
    "test": "jest",
    "coverage": "jest --coverage",
    "report": "npm run coverage && if [[ $OSTYPE == darwin* ]]; then open=open; else open=xdg-open; fi && $open coverage/lcov-report/index.html",
    "update": "updates -u && npm install && npm audit fix && npm test"
  },
  "main": "lib/SealError.js",
  "dependencies": {
    "v8-callsites": "1.2.1"
  },
  "devDependencies": {
    "@sealsystems/eslint-config-es": "2.0.2",
    "eslint": "5.9.0",
    "jest": "23.6.0",
    "updates": "5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sealsystems/node-error.git"
  },
  "release": {
    "extends": "@sealsystems/semantic-release-config/node-module"
  },
  "publishConfig": {
    "access": "public"
  }
}
